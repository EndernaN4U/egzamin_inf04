<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="assets\obraz.png" />
    <None Remove="assets\obraz2.png" />
    <None Remove="assets\obraz3.png" />
    <None Remove="data\Data.txt" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="assets\obraz.png" />
    <Resource Include="assets\obraz2.png" />
    <Resource Include="assets\obraz3.png" />
    <Content Include="data\Data.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <CustomToolNamespace></CustomToolNamespace>
    </Content>
  </ItemGroup>

</Project>
